<html>
<head>
	<title><%= @page_title || "GWAP2" %></title>
	<%= javascript_include_tag "jquery-1.3.2.min" %>
	<%= javascript_include_tag "superfish" %>
	<%= javascript_include_tag "prototype" %>

	<%= javascript_include_tag "jquery.bgiframe.min" %>	
	<%= javascript_include_tag "jquery.autocomplete.pack" %>
	<%= javascript_include_tag "jquery-ui-1.7.1.custom.min" %>

	<script type="text/javascript" src="http://www.google.com/jsapi"></script>     
	
	
	<%= javascript_include_tag "gwap2" %>   
	
	<script type="text/javascript" src="../../js/bionetwork.js"></script>
	<script type="text/javascript" src="../../js/bioheatmap.js"></script>
	

	<%= stylesheet_link_tag "superfish" %>
	<%= stylesheet_link_tag "spiderweb" %>
	<%= stylesheet_link_tag "jquery.autocomplete" %>
	<%= stylesheet_link_tag "gwap2" %>

</head>
<body>           
<div id="top"></div>
<div id="page">  
    <% if logged_in() %>
        <p>Logged in as <i><%= email() %></i> | <a href="<%= url_for :action => "logout"%>">Log Out </p>
    <% else %>
        <p>Not logged in</p>
    <% end %>
<%= yield :layout %> 

</div>
</body>
</html>
